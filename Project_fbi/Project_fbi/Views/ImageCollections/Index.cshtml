@model IEnumerable<Project_fbi.ImageCollection>

@{
    ViewBag.Title = "Display";
}

@using (Html.BeginForm())
{
    <table border="1">
        <thead>
            <tr>
                <th>Image</th>
             ]            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td><img src="~/images/@item.ImageName" width="100" height="100" /></td>
                </tr>
            }
        </tbody>
    </table>
}